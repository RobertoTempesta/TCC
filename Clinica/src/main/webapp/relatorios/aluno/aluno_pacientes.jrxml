	<queryString language="SQL">
		<![CDATA[SELECT pessoa.nome,
	aluno.RA,
	aluno.dataCadastro,
	(SELECT nome FROM pessoa, supervisor WHERE supervisor.codigo = aluno.supervisor_codigo AND pessoa.codigo = supervisor.pessoa_codigo) "supervisor",
	funcao.descricao
FROM aluno
